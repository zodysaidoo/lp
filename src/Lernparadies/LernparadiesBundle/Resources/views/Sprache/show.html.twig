{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Sprache</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Sprachenewdate</th>
                <td>{{ entity.spracheNewdate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Spracheeditdate</th>
                <td>{{ entity.spracheEditdate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Sprachename</th>
                <td>{{ entity.spracheName }}</td>
            </tr>
            <tr>
                <th>Sprachecode</th>
                <td>{{ entity.spracheCode }}</td>
            </tr>
            <tr>
                <th>Spracheaktiv</th>
                <td>{{ entity.spracheAktiv }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('sprache') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('sprache_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
